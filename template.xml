{{- $borderColor := (datasource "data").borderColor -}}
{{- $colorNames := slice "white" "red" "orange" "yellow" "green" "darkgreen" "darkblue" "blue" }}
{{- $alternateFlag := (datasource "data").alternate -}}
{{- $altBgColors := eq $alternateFlag "on" | ternary (slice "343D4F" "332C3D" "34343E" "333C40" "263C41" "22363D" "23324C" "23384F") $alternateFlag -}}
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<a:tblStyleLst xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main" def="{0817EA92-75D0-4044-A80A-286907CE0000}">
	{{- range $idxTint, $tinting := split (conv.ToString (datasource "data").tinting) " " -}}
	{{- range $idxOuter, $outerBorder := split (conv.ToString (datasource "data").outerBorder) " " -}}
	{{- range $idxInner, $innerBorder := split (conv.ToString (datasource "data").innerBorder) " " -}}
	{{- range $idxColor, $color := split ((datasource "data").colors) " " }}
	<a:tblStyle styleId="{0817EA92-75D0-4044-A80A-286907CE{{ $idxTint }}{{ $idxColor }}{{ $idxInner }}{{ $idxOuter }}}" styleName="{{ index $colorNames $idxColor }}{{ eq $tinting "0" | ternary "" "-tinted" }}{{ eq $innerBorder "0" | ternary "" "-inner" }}{{ eq $outerBorder "0" | ternary "" "-outer" }}">
		<a:wholeTbl>
			<a:tcTxStyle>
				<a:fontRef idx="minor">
					<a:prstClr val="black"/>
				</a:fontRef>
				<a:schemeClr val="tx1"/>
			</a:tcTxStyle>
			<a:tcStyle>
				<a:tcBdr>
					<a:left>
						<a:ln w="{{ $outerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ eq $borderColor "" | ternary $color $borderColor }}"/>
							</a:solidFill>
						</a:ln>
					</a:left>
					<a:right>
						<a:ln w="{{ $outerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ eq $borderColor "" | ternary $color $borderColor }}"/>
							</a:solidFill>
						</a:ln>
					</a:right>
					<a:top>
						<a:ln w="{{ $outerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ eq $borderColor "" | ternary $color $borderColor }}"/>
							</a:solidFill>
						</a:ln>
					</a:top>
					<a:bottom>
						<a:ln w="{{ $outerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ eq $borderColor "" | ternary $color $borderColor }}"/>
							</a:solidFill>
						</a:ln>
					</a:bottom>
					<a:insideH>
						<a:ln w="{{ $innerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ eq $borderColor "" | ternary $color $borderColor }}"/>
							</a:solidFill>
						</a:ln>
					</a:insideH>
					<a:insideV>
						<a:ln w="{{ $innerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ eq $borderColor "" | ternary $color $borderColor }}"/>
							</a:solidFill>
						</a:ln>
					</a:insideV>
				</a:tcBdr>
				<a:fill>
					<a:noFill/>
				</a:fill>
			</a:tcStyle>
		</a:wholeTbl>
		{{- if not (eq $alternateFlag "off") -}}
		<a:band1H>
			<a:tcStyle>
				<a:tcBdr/>
				<a:fill>
					<a:solidFill>
						<a:srgbClr val="{{ if eq $tinting "0" }}{{ index $altBgColors 0 }}{{ else }}{{ index $altBgColors $idxColor }}{{ end }}"/>
					</a:solidFill>
				</a:fill>
			</a:tcStyle>
		</a:band1H>
		{{- if false -}}
		<a:band2H>
			<a:tcStyle>
				<a:tcBdr/>
			</a:tcStyle>
		</a:band2H>
		<a:band1V>
			<a:tcStyle>
				<a:tcBdr/>
				<a:fill>
					<a:noFill/>
				</a:fill>
			</a:tcStyle>
		</a:band1V>
		<a:band2V>
			<a:tcStyle>
				<a:tcBdr/>
			</a:tcStyle>
		</a:band2V>
		{{- end }}
		{{- end }}
		<a:lastCol>
			<a:tcTxStyle b="on">
				<a:fontRef idx="minor">
					<a:prstClr val="black"/>
				</a:fontRef>
				<a:schemeClr val="{{ $color }}"/>
			</a:tcTxStyle>
		</a:lastCol>
		<a:firstCol>
			<a:tcTxStyle b="{{ if eq $color "tx1"}}on{{ else }}off{{ end }}">
				<a:fontRef idx="minor">
					<a:prstClr val="black"/>
				</a:fontRef>
				<a:schemeClr val="{{ $color }}"/>
			</a:tcTxStyle>
		</a:firstCol>
		<a:lastRow>
			<a:tcTxStyle b="on">
				<a:fontRef idx="minor">
					<a:prstClr val="black"/>
				</a:fontRef>
				<a:schemeClr val="{{ $color }}"/>
			</a:tcTxStyle>
			<a:tcStyle>
				<a:tcBdr>
					{{- if not (eq $outerBorder "0") }}
					<a:left>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:left>
					<a:right>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:right>
					{{- end }}
					<a:top>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:top>
					{{- if not (eq $outerBorder "0") }}
					<a:bottom>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:bottom>
					{{- end }}
					{{- if not (eq $innerBorder "0") }}
					<a:insideH>
						<a:ln w="{{ $innerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:insideH>
					{{- end }}
				</a:tcBdr>
			</a:tcStyle>
		</a:lastRow>
		<a:firstRow>
			<a:tcTxStyle b="on">
				<a:fontRef idx="minor">
					<a:prstClr val="black"/>
				</a:fontRef>
				<a:schemeClr val="{{ $color }}"/>
			</a:tcTxStyle>
			<a:tcStyle>
				<a:tcBdr>
					{{- if not (eq $outerBorder "0") }}
					<a:left>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:left>
					<a:right>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:right>
					{{- end }}
					<a:top>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:top>
					<a:bottom>
						<a:ln w="31750" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:bottom>
					{{- if not (eq $innerBorder "0") }}
					<a:insideH>
						<a:ln w="{{ $innerBorder }}" cmpd="sng">
							<a:solidFill>
								<a:schemeClr val="{{ $color }}"/>
							</a:solidFill>
						</a:ln>
					</a:insideH>
					{{- end }}
				</a:tcBdr>
			</a:tcStyle>
		</a:firstRow>
	</a:tblStyle>
	{{- end }}
	{{- end }}
	{{- end }}
	{{- end }}
</a:tblStyleLst>
